<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/mapFragment2"
        android:name="dev.anonymous.eilaji.ui.map.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />


    <fragment
        android:id="@+id/navigation_home"
        android:name="dev.anonymous.eilaji.ui.user_interface.home.HomeFragment"
        android:label="@string/delivery_to_your_location"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="dev.anonymous.eilaji.ui.user_interface.dashboard.DashboardFragment"
        android:label="@string/chats"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="dev.anonymous.eilaji.ui.user_interface.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_categories"
        android:name="dev.anonymous.eilaji.ui.user_interface.categories.CategoriesFragment"
        android:label="@string/categories"
        tools:layout="@layout/fragment_categories" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="dev.anonymous.eilaji.ui.user_interface.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

</navigation>